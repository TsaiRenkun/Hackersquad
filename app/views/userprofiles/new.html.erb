<%= form_with scope: :userprofile, url: userprofiles_path, local: true do |form| %>
  <p>
    <%= form.label :username %><br>
    <%= form.text_field :username %>
  </p>

  <p>
    <%= form.label :description %><br>
    <%= form.text_field :description %>
  </p>

    <p>
    <%= form.label :img %><br>
    <%= form.text_field :img %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
